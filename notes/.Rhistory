V0 = 2000 # eV
hc = 200 # nm
## array of E values
Evalues = seq(0,2000,length.out = 2000)
## initialise array of determinant values
detvalues=NULL
i=1
for (E in Evalues) {
## set k and q
k = sqrt((2*mc2*E)/(hc^2))
q = sqrt((2*mc2*(V0-E))/(hc^2))
ka2 = k*a/2
qa2 = -q*a/2
## build matrix
M = matrix(c(-sin(ka2), cos(ka2), -exp(qa2), 0,
sin(ka2), cos(ka2), 0, -exp(qa2),
k*cos(ka2), k*sin(ka2), -q*exp(qa2), 0,
k*cos(ka2), -k*sin(ka2), 0, q*exp(qa2)), 4, 4, byrow=TRUE)
## compute determinant
detvalues[i] = det(M)
i=i+1
}
